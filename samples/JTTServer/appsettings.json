{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "SystemConfig": {
    "ProjectName": "JTT协议TCP服务",
    "LoggerType": "Console",

    //登录验证信息
    "GnsscenterID": [ 1, 2, 3 ],
    "Password": "123456",

    //TCP
    "ServerName": "JTTServer",
    "ServerIP": "Any",
    "ServerPort": 8066,
    "ServerBackLog": 10000,

    //JTT
    "JTTConfigFilePath": "jsonconfig/jtt809-protocol.json",
    "JTTVersion": "JTT809",
    "JTT809VersionFlag": [ 1, 0, 0 ],

    //启用转发（可以利用转发功能来做本地调试）
    "EnableForward": true
  }
}
